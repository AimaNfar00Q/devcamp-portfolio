<!DOCTYPE html>
<html>
  <head>
    <title><%= @page_title %> </title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
   
    <meta name="keywords" content= "<%= @seo_keywords %>" />

    <%= stylesheet_link_tag 'blogs', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  
  </head>

  <body>
        <p class="notice"><%= notice %></p>
       <p class="alert"><%= alert %></p>

      

  <%= login_helper %>  <!-- calling helper from app-helper-app-helper.rb -->
    <%= yield %>
    <%= render "shared/nav", localize:'bottom' %>  <!-- calling partial from views-shared-nav -->

 
 <%if session[:source] %>
  <p> Thanks for visiting me from   <%= session[:source] %> </p>
<%end%>
  </body>
</html>
